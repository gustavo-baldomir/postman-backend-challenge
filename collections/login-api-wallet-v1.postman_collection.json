		{
			"name": "Login Exitoso",
			"item": [
				{
					"name": "[TC-001] Login Exitoso - Usuario & Contraseña VALIDO",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"pm.environment.set(\"access_token\", pm.response.json().access_token);\r",
									"\r",
									"pm.test(\"Status code 200 OK - Login exitoso - Usuario & Contraseña VALIDO\", function () {\r",
									"    pm.response.to.have.status(200);\r",
									"});\r",
									"\r",
									"const Ajv = require('ajv');\r",
									"const ajv = new Ajv({ allErrors: true, strict: true });\r",
									"\r",
									"const schema = {\r",
									"    type: \"object\",\r",
									"    properties: {\r",
									"        access_token: { type: \"string\" },\r",
									"        token_type: { \r",
									"            type: \"string\",\r",
									"            enum: [\"bearer\", \"Bearer\"] \r",
									"        },\r",
									"        access_token_expires: { type: \"number\" },\r",
									"        tarjetas: {\r",
									"            type: \"array\",\r",
									"            items: {\r",
									"                type: \"object\",\r",
									"                properties: {\r",
									"                    descripcion: { type: \"string\" },\r",
									"                    numero: { \r",
									"                        type: \"string\",\r",
									"                        pattern: \"^\\\\d{12}$\"\r",
									"                    }\r",
									"                },\r",
									"                required: [\r",
									"                    \"descripcion\",\r",
									"                    \"numero\"\r",
									"                ],\r",
									"                additionalProperties: false\r",
									"            }\r",
									"        }\r",
									"    },\r",
									"    required: [\r",
									"        \"access_token\",\r",
									"        \"token_type\",\r",
									"        \"access_token_expires\",\r",
									"        \"tarjetas\"\r",
									"    ],\r",
									"    additionalProperties: false\r",
									"};\r",
									"\r",
									"const response = pm.response.json();\r",
									"const validate = ajv.compile(schema);\r",
									"const valid = validate(response);\r",
									"\r",
									"pm.test(\"JSON Schema VALIDO\", function () {\r",
									"    pm.expect(valid, JSON.stringify(validate.errors, null, 2)).to.be.true;\r",
									"});\r",
									"\r",
									"if (!valid) {\r",
									"    console.error(\"Errores de validación del schema:\", validate.errors);\r",
									"}\r",
									"\r",
									"/// TEST: Validar que el PATH de la solicitud sea /wallet/sesion ///\r",
									"\r",
									"pm.test(\"El PATH de la solicitud es CORRECTO\", function () {\r",
									"    const expectedPath = \"/wallet/sesion\";\r",
									"\r",
									"    // Obtenemos la URL completa y extraemos el path\r",
									"    const requestUrl = pm.request.url;\r",
									"    const actualPath = \"/\" + requestUrl.path.join(\"/\");\r",
									"\r",
									"    pm.expect(actualPath).to.eql(expectedPath);\r",
									"});\r",
									"\r",
									""
								],
								"type": "text/javascript",
								"packages": {}
							}
						}
